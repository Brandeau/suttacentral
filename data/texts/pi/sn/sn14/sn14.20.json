  "sn14.20:1.1": "Saṃyutta Nikāya 14",
  "sn14.20:2.1": "2. Dutiyavagga",
  "sn14.20:3.1": "<span class=\\\"var\\\" title=\\\"anottāpamūlakatikattayasuttaṃ (bj)\\\" id=\\\"note171\\\">20. Anottappamūlakasutta</span>",
  "sn14.20:4.1": "Sāvatthiyaṃ viharati.",
  "sn14.20:4.2": "“Dhātusova, bhikkhave, sattā saṃsandanti samenti.",
  "sn14.20:4.3": "Anottappino anottappīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:4.4": "appassutā appassutehi saddhiṃ saṃsandanti samenti;",
  "sn14.20:4.5": "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;",
  "sn14.20:4.6": "ottappino ottappīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:4.7": "bahussutā bahussutehi saddhiṃ saṃsandanti samenti;",
  "sn14.20:4.8": "paññavanto paññavantehi saddhiṃ saṃsandanti samenti … pe ….",
  "sn14.20:5.1": "Anottappino anottappīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:5.2": "kusītā kusītehi saddhiṃ saṃsandanti samenti;",
  "sn14.20:5.3": "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;",
  "sn14.20:5.4": "ottappino ottappīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:5.5": "āraddhavīriyā āraddhavīriyehi saddhiṃ saṃsandanti samenti;",
  "sn14.20:5.6": "paññavanto paññavantehi saddhiṃ saṃsandanti samenti … pe ….",
  "sn14.20:5.7": "<span class=\\\"brnum\\\">(2)</span>",
  "sn14.20:6.1": "Anottappino anottappīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:6.2": "muṭṭhassatino muṭṭhassatīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:6.3": "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;",
  "sn14.20:6.4": "ottappino ottappīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:6.5": "upaṭṭhitassatino upaṭṭhitassatīhi saddhiṃ saṃsandanti samenti;",
  "sn14.20:6.6": "paññavanto paññavantehi saddhiṃ saṃsandanti samentī”ti … pe ….",
  "sn14.20:6.7": "<span class=\\\"brnum\\\">(3)</span>",
  "sn14.20:7.1": "Dasamaṃ."
}