  "sn14.29:1.1": "Saṃyutta Nikāya 14",
  "sn14.29:2.1": "3. Kammapathavagga",
  "sn14.29:3.1": "29. Dasaṅgasutta",
  "sn14.29:4.1": "Sāvatthiyaṃ viharati.",
  "sn14.29:4.2": "“Dhātusova, bhikkhave, sattā saṃsandanti samenti.",
  "sn14.29:4.3": "Micchādiṭṭhikā micchādiṭṭhikehi saddhiṃ saṃsandanti samenti;",
  "sn14.29:4.4": "micchāsaṅkappā … pe …",
  "sn14.29:4.5": "micchāvācā …",
  "sn14.29:4.6": "micchākammantā …",
  "sn14.29:4.7": "micchāājīvā …",
  "sn14.29:4.8": "micchāvāyāmā …",
  "sn14.29:4.9": "micchāsatino …",
  "sn14.29:4.10": "micchāsamādhino micchāsamādhīhi saddhiṃ saṃsandanti samenti;",
  "sn14.29:4.11": "micchāñāṇino micchāñāṇīhi saddhiṃ saṃsandanti samenti;",
  "sn14.29:4.12": "micchāvimuttino micchāvimuttīhi saddhiṃ saṃsandanti samenti.",
  "sn14.29:5.1": "Sammādiṭṭhikā sammādiṭṭhikehi saddhiṃ saṃsandanti samenti;",
  "sn14.29:5.2": "sammāsaṅkappā … pe …",
  "sn14.29:5.3": "sammāvācā …",
  "sn14.29:5.4": "sammākammantā …",
  "sn14.29:5.5": "sammāājīvā …",
  "sn14.29:5.6": "sammāvāyāmā …",
  "sn14.29:5.7": "sammāsatino …",
  "sn14.29:5.8": "sammāsamādhino …",
  "sn14.29:5.9": "sammāñāṇino sammāñāṇīhi saddhiṃ saṃsandanti samenti;",
  "sn14.29:5.10": "sammāvimuttino sammāvimuttīhi saddhiṃ saṃsandanti samentī”ti.",
  "sn14.29:6.1": "Sattamaṃ.",
  "sn14.29:7.1": "(Sabbattha atītānāgatapaccuppannaṃ kātabbaṃ.)",
  "sn14.29:8.1": "Asamāhitaṃ dussīlaṃ,",
  "sn14.29:8.2": "pañca sikkhāpadāni ca;",
  "sn14.29:8.3": "Satta kammapathā vuttā,",
  "sn14.29:8.4": "dasakammapathena ca;",
  "sn14.29:8.5": "Chaṭṭhaṃ aṭṭhaṅgiko vutto,",
  "sn14.29:8.6": "dasaṅgena ca sattamaṃ.",
  "sn14.29:9.1": "Kammapathavaggo tatiyo."
}