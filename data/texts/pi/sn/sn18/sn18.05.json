  "sn18.5:1.1": "Saṃyutta Nikāya 18",
  "sn18.5:2.1": "1. Paṭhamavagga",
  "sn18.5:3.1": "5. Vedanāsutta",
  "sn18.5:4.1": "Sāvatthiyaṃ viharati.",
  "sn18.5:4.2": "“Taṃ kiṃ maññasi, rāhula, cakkhusamphassajā vedanā niccā vā aniccā vā”ti?",
  "sn18.5:4.3": "“Aniccā, bhante” …",
  "sn18.5:4.4": "“sotasamphassajā vedanā … pe …",
  "sn18.5:4.5": "ghānasamphassajā vedanā …",
  "sn18.5:4.6": "jivhāsamphassajā vedanā …",
  "sn18.5:4.7": "kāyasamphassajā vedanā …",
  "sn18.5:4.8": "manosamphassajā vedanā niccā vā aniccā vā”ti?",
  "sn18.5:4.9": "“Aniccā, bhante” …",
  "sn18.5:4.10": "“evaṃ passaṃ, rāhula, sutavā ariyasāvako cakkhusamphassajāya vedanāyapi nibbindati … pe …",
  "sn18.5:4.11": "sota …",
  "sn18.5:4.12": "ghāna …",
  "sn18.5:4.13": "jivhā …",
  "sn18.5:4.14": "kāya …",
  "sn18.5:4.15": "manosamphassajāya vedanāyapi nibbindati … pe …",
  "sn18.5:4.16": "pajānātī”ti.",
  "sn18.5:5.1": "Pañcamaṃ."
}