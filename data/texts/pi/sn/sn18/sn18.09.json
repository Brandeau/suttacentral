  "sn18.9:1.1": "Saṃyutta Nikāya 18",
  "sn18.9:2.1": "1. Paṭhamavagga",
  "sn18.9:3.1": "9. Dhātusutta",
  "sn18.9:4.1": "Sāvatthiyaṃ viharati.",
  "sn18.9:4.2": "“Taṃ kiṃ maññasi, rāhula, pathavīdhātu niccā vā aniccā vā”ti?",
  "sn18.9:4.3": "“Aniccā, bhante” …",
  "sn18.9:4.4": "“āpodhātu … pe …",
  "sn18.9:4.5": "tejodhātu …",
  "sn18.9:4.6": "vāyodhātu …",
  "sn18.9:4.7": "ākāsadhātu …",
  "sn18.9:4.8": "viññāṇadhātu niccā vā aniccā vā”ti?",
  "sn18.9:4.9": "“Aniccā, bhante” …",
  "sn18.9:4.10": "“evaṃ passaṃ, rāhula, sutavā ariyasāvako pathavīdhātuyāpi nibbindati … pe …",
  "sn18.9:4.11": "āpodhātuyāpi nibbindati …",
  "sn18.9:4.12": "tejodhātuyāpi nibbindati …",
  "sn18.9:4.13": "vāyodhātuyāpi nibbindati …",
  "sn18.9:4.14": "ākāsadhātuyāpi nibbindati …",
  "sn18.9:4.15": "viññāṇadhātuyāpi nibbindati … pe …",
  "sn18.9:4.16": "pajānātī”ti.",
  "sn18.9:5.1": "Navamaṃ."
}