  "sn22.139:1.1": "Saṃyutta Nikāya 22",
  "sn22.139:2.1": "14. Kukkuḷavagga",
  "sn22.139:3.1": "139. Tatiyaaniccasutta",
  "sn22.139:4.1": "Sāvatthinidānaṃ.",
  "sn22.139:4.2": "“Yaṃ, bhikkhave, aniccaṃ;",
  "sn22.139:4.3": "tatra vo chandarāgo pahātabbo.",
  "sn22.139:4.4": "Kiñca, bhikkhave, aniccaṃ?",
  "sn22.139:4.5": "Rūpaṃ, bhikkhave, aniccaṃ, tatra vo chandarāgo pahātabbo.",
  "sn22.139:4.6": "Vedanā aniccā …",
  "sn22.139:4.7": "saññā …",
  "sn22.139:4.8": "saṅkhārā …",
  "sn22.139:4.9": "viññāṇaṃ aniccaṃ;",
  "sn22.139:4.10": "tatra vo chandarāgo pahātabbo.",
  "sn22.139:4.11": "Yaṃ, bhikkhave, aniccaṃ;",
  "sn22.139:4.12": "tatra vo chandarāgo pahātabbo”ti.",
  "sn22.139:5.1": "Catutthaṃ."
}