  "sn22.144:1.1": "Saṃyutta Nikāya 22",
  "sn22.144:2.1": "14. Kukkuḷavagga",
  "sn22.144:3.1": "144. Dutiyaanattasutta",
  "sn22.144:4.1": "Sāvatthinidānaṃ.",
  "sn22.144:4.2": "“Yo, bhikkhave, anattā;",
  "sn22.144:4.3": "tatra vo rāgo pahātabbo.",
  "sn22.144:4.4": "Ko ca, bhikkhave, anattā?",
  "sn22.144:4.5": "Rūpaṃ, bhikkhave, anattā;",
  "sn22.144:4.6": "tatra vo rāgo pahātabbo.",
  "sn22.144:4.7": "Vedanā anattā …",
  "sn22.144:4.8": "saññā …",
  "sn22.144:4.9": "saṅkhārā …",
  "sn22.144:4.10": "viññāṇaṃ anattā;",
  "sn22.144:4.11": "tatra vo rāgo pahātabbo.",
  "sn22.144:4.12": "Yo, bhikkhave, anattā;",
  "sn22.144:4.13": "tatra vo rāgo pahātabbo”ti.",
  "sn22.144:5.1": "Navamaṃ."
}