  "sn22.143:1.1": "Saṃyutta Nikāya 22",
  "sn22.143:2.1": "14. Kukkuḷavagga",
  "sn22.143:3.1": "143. Anattasutta",
  "sn22.143:4.1": "Sāvatthinidānaṃ.",
  "sn22.143:4.2": "“Yo, bhikkhave, anattā;",
  "sn22.143:4.3": "tatra vo chando pahātabbo.",
  "sn22.143:4.4": "Ko ca, bhikkhave, anattā?",
  "sn22.143:4.5": "Rūpaṃ, bhikkhave, anattā;",
  "sn22.143:4.6": "tatra vo chando pahātabbo.",
  "sn22.143:4.7": "Vedanā anattā …",
  "sn22.143:4.8": "saññā …",
  "sn22.143:4.9": "saṅkhārā …",
  "sn22.143:4.10": "viññāṇaṃ anattā;",
  "sn22.143:4.11": "tatra vo chando pahātabbo.",
  "sn22.143:4.12": "Yo, bhikkhave, anattā;",
  "sn22.143:4.13": "tatra vo chando pahātabbo”ti.",
  "sn22.143:5.1": "Aṭṭhamaṃ."
}