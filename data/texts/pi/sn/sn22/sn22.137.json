  "sn22.137:1.1": "Saṃyutta Nikāya 22",
  "sn22.137:2.1": "14. Kukkuḷavagga",
  "sn22.137:3.1": "137. Aniccasutta",
  "sn22.137:4.1": "Sāvatthinidānaṃ.",
  "sn22.137:4.2": "“Yaṃ, bhikkhave, aniccaṃ;",
  "sn22.137:4.3": "tatra vo chando pahātabbo.",
  "sn22.137:4.4": "Kiñca, bhikkhave, aniccaṃ?",
  "sn22.137:4.5": "Rūpaṃ, bhikkhave, aniccaṃ;",
  "sn22.137:4.6": "tatra vo chando pahātabbo.",
  "sn22.137:4.7": "Vedanā aniccā … pe …",
  "sn22.137:4.8": "saññā …",
  "sn22.137:4.9": "saṅkhārā …",
  "sn22.137:4.10": "viññāṇaṃ aniccaṃ;",
  "sn22.137:4.11": "tatra vo chando pahātabbo.",
  "sn22.137:4.12": "Yaṃ, bhikkhave, aniccaṃ;",
  "sn22.137:4.13": "tatra vo chando pahātabbo”ti.",
  "sn22.137:5.1": "Dutiyaṃ."
}