  "sn22.145:1.1": "Saṃyutta Nikāya 22",
  "sn22.145:2.1": "14. Kukkuḷavagga",
  "sn22.145:3.1": "145. Tatiyaanattasutta",
  "sn22.145:4.1": "Sāvatthinidānaṃ.",
  "sn22.145:4.2": "“Yo, bhikkhave, anattā;",
  "sn22.145:4.3": "tatra vo chandarāgo pahātabbo.",
  "sn22.145:4.4": "Ko ca, bhikkhave, anattā?",
  "sn22.145:4.5": "Rūpaṃ, bhikkhave, anattā;",
  "sn22.145:4.6": "tatra vo chandarāgo pahātabbo.",
  "sn22.145:4.7": "Vedanā anattā …",
  "sn22.145:4.8": "saññā …",
  "sn22.145:4.9": "saṅkhārā …",
  "sn22.145:4.10": "viññāṇaṃ anattā;",
  "sn22.145:4.11": "tatra vo chandarāgo pahātabbo.",
  "sn22.145:4.12": "Yo, bhikkhave, anattā;",
  "sn22.145:4.13": "tatra vo chandarāgo pahātabbo”ti.",
  "sn22.145:5.1": "Dasamaṃ."
}