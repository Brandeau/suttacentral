  "sn48.31:2.1": "4. Sukhindriyavagga",
  "sn48.31:3.1": "<span class=\\\"var\\\" title=\\\"suddhakasuttaṃ (bj)\\\" id=\\\"note244\\\">31. Suddhikasutta</span>",
