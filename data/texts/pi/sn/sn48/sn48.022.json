  "sn48.22:2.1": "3. Chaḷindriyavagga",
  "sn48.22:3.1": "<span class=\\\"var\\\" title=\\\"jīvita (pts1)\\\" id=\\\"note235\\\">22. Jīvitindriyasutta</span>",
