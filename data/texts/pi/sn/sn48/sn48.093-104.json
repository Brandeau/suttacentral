  "sn48.93-104:1.1": "Saṃyutta Nikāya 48",
  "sn48.93-104:2.1": "10. Balakaraṇīyavagga",
  "sn48.93-104:3.1": "93. Balakaraṇīyavagga",
  "sn48.93-104:4.1": "(Balakaraṇīyavaggo vitthāretabbo.)",
  "sn48.93-104:5.1": "Balaṃ bījañca nāgo ca,",
  "sn48.93-104:5.2": "rukkho kumbhena sūkiyā;",
  "sn48.93-104:5.3": "Ākāsena ca dve meghā,",
  "sn48.93-104:5.4": "nāvā āgantukā nadīti."
}