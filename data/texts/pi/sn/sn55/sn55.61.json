  "sn55.61:1.1": "Saṃyutta Nikāya 55",
  "sn55.61:2.1": "6. Sappaññavagga",
  "sn55.61:3.1": "<span class=\\\"var\\\" title=\\\"vepullasuttaṃ (bj) | vepullatā (pts1)\\\" id=\\\"note497\\\">61. Paññāvepullasutta</span>",
  "sn55.61:4.1": "“….",
  "sn55.61:4.2": "Paññāvepullāya …",
  "sn55.61:4.3": "saṃvattantī”ti.",
  "sn55.61:5.1": "Ekādasamaṃ.",
  "sn55.61:6.1": "Sappaññavaggo chaṭṭho.",
