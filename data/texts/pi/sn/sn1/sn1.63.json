  "sn1.63:1.1": "Saṃyutta Nikāya 1",
  "sn1.63:2.1": "7. Addhavagga",
  "sn1.63:3.1": "63. Taṇhāsutta",
  "sn1.63:4.1": "“Kenassu nīyati loko,",
  "sn1.63:4.2": "kenassu parikassati;",
  "sn1.63:4.3": "Kissassu ekadhammassa,",
  "sn1.63:4.4": "sabbeva vasamanvagū”ti.",
  "sn1.63:5.1": "“Taṇhāya nīyati loko,",
  "sn1.63:5.2": "taṇhāya parikassati;",
  "sn1.63:5.3": "Taṇhāya ekadhammassa,",
  "sn1.63:5.4": "sabbeva vasamanvagū”ti."
