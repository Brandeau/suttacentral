  "sn1.14:1.1": "Saṃyutta Nikāya 1",
  "sn1.14:2.1": "2. Nandanavagga",
  "sn1.14:3.1": "14. Khattiyasutta",
  "sn1.14:4.1": "“Khattiyo dvipadaṃ seṭṭho,",
  "sn1.14:4.2": "<span class=\\\"var\\\" title=\\\"balivaddo (bj, pts1-2) | balibaddho (s1-3, km) | balibaddo (mr)\\\" id=\\\"note17\\\">balībaddo</span> catuppadaṃ;",
  "sn1.14:4.3": "Komārī seṭṭhā bhariyānaṃ,",
  "sn1.14:4.4": "yo ca puttāna pubbajo”ti.",
  "sn1.14:5.1": "“Sambuddho dvipadaṃ seṭṭho,",
  "sn1.14:5.2": "ājānīyo catuppadaṃ;",
  "sn1.14:5.3": "Sussūsā seṭṭhā bhariyānaṃ,",
  "sn1.14:5.4": "yo ca puttānamassavo”ti."
}