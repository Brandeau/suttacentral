  "sn1.57:1.1": "Saṃyutta Nikāya 1",
  "sn1.57:2.1": "6. Jarāvagga",
  "sn1.57:3.1": "<span class=\\\"var\\\" title=\\\"tatiyajanetisuttaṃ (bj)\\\" id=\\\"note90\\\">57. Tatiyajanasutta</span>",
  "sn1.57:4.1": "“Kiṃsu janeti purisaṃ,",
  "sn1.57:4.2": "kiṃsu tassa vidhāvati;",
  "sn1.57:4.3": "Kiṃsu saṃsāramāpādi,",
  "sn1.57:4.4": "kiṃsu tassa parāyanan”ti.",
  "sn1.57:5.1": "“Taṇhā janeti purisaṃ,",
  "sn1.57:5.2": "cittamassa vidhāvati;",
  "sn1.57:5.3": "Satto saṃsāramāpādi,",
  "sn1.57:5.4": "kammaṃ tassa parāyanan”ti."
}