  "sn1.55:1.1": "Saṃyutta Nikāya 1",
  "sn1.55:2.1": "6. Jarāvagga",
  "sn1.55:3.1": "<span class=\\\"var\\\" title=\\\"paṭhamajanetisuttaṃ (bj)\\\" id=\\\"note88\\\">55. Paṭhamajanasutta</span>",
  "sn1.55:4.1": "“Kiṃsu janeti purisaṃ,",
  "sn1.55:4.2": "kiṃsu tassa vidhāvati;",
  "sn1.55:4.3": "Kiṃsu saṃsāramāpādi,",
  "sn1.55:4.4": "kiṃsu tassa mahabbhayan”ti.",
  "sn1.55:5.1": "“Taṇhā janeti purisaṃ,",
  "sn1.55:5.2": "cittamassa vidhāvati;",
  "sn1.55:5.3": "Satto saṃsāramāpādi,",
  "sn1.55:5.4": "dukkhamassa mahabbhayan”ti."
}