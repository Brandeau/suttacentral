  "sn1.69:1.1": "Saṃyutta Nikāya 1",
  "sn1.69:2.1": "7. Addhavagga",
  "sn1.69:3.1": "69. Icchāsutta",
  "sn1.69:4.1": "“Kenassu bajjhatī loko,",
  "sn1.69:4.2": "kissa vinayāya muccati;",
  "sn1.69:4.3": "Kissassu vippahānena,",
  "sn1.69:4.4": "sabbaṃ chindati bandhanan”ti.",
  "sn1.69:5.1": "“Icchāya bajjhatī loko,",
  "sn1.69:5.2": "icchāvinayāya muccati;",
  "sn1.69:5.3": "Icchāya vippahānena,",
  "sn1.69:5.4": "sabbaṃ chindati bandhanan”ti."
}