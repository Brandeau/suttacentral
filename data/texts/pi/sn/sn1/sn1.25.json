  "sn1.25:1.1": "Saṃyutta Nikāya 1",
  "sn1.25:2.1": "3. Sattivagga",
  "sn1.25:3.1": "25. Arahantasutta",
  "sn1.25:4.1": "“Yo hoti bhikkhu arahaṃ katāvī,",
  "sn1.25:4.2": "Khīṇāsavo antimadehadhārī;",
  "sn1.25:4.3": "Ahaṃ vadāmītipi so vadeyya,",
  "sn1.25:4.4": "Mamaṃ vadantītipi so vadeyyā”ti.",
  "sn1.25:5.1": "“Yo hoti bhikkhu arahaṃ katāvī,",
  "sn1.25:5.2": "Khīṇāsavo antimadehadhārī;",
  "sn1.25:5.3": "Ahaṃ vadāmītipi so vadeyya,",
  "sn1.25:5.4": "Mamaṃ vadantītipi so vadeyya;",
  "sn1.25:5.5": "Loke samaññaṃ kusalo viditvā,",
  "sn1.25:5.6": "Vohāramattena <span class=\\\"var\\\" title=\\\"sa (pts2)\\\" id=\\\"note43\\\">so</span> vohareyyā”ti.",
  "sn1.25:6.1": "“Yo hoti bhikkhu arahaṃ katāvī,",
  "sn1.25:6.2": "Khīṇāsavo antimadehadhārī;",
  "sn1.25:6.3": "Mānaṃ nu kho so upagamma bhikkhu,",
  "sn1.25:6.4": "Ahaṃ vadāmītipi so vadeyya;",
  "sn1.25:6.5": "Mamaṃ vadantītipi so vadeyyā”ti.",
  "sn1.25:7.1": "“Pahīnamānassa na santi ganthā,",
  "sn1.25:7.2": "Vidhūpitā mānaganthassa sabbe;",
  "sn1.25:7.3": "Sa vītivatto <span class=\\\"var\\\" title=\\\"maññanaṃ (bj) | yamataṃ (s1-3, pts1-2) | maññitaṃ (?)\\\" id=\\\"note44\\\">maññataṃ</span> sumedho,",
  "sn1.25:7.4": "Ahaṃ vadāmītipi so vadeyya.",
  "sn1.25:8.1": "Mamaṃ vadantītipi so vadeyya,",
  "sn1.25:8.2": "Loke samaññaṃ kusalo viditvā;",
  "sn1.25:8.3": "Vohāramattena so vohareyyā”ti."
}