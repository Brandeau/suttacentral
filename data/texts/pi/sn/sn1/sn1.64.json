  "sn1.64:1.1": "Saṃyutta Nikāya 1",
  "sn1.64:2.1": "7. Addhavagga",
  "sn1.64:3.1": "64. Saṃyojanasutta",
  "sn1.64:4.1": "“Kiṃsu saṃyojano loko,",
  "sn1.64:4.2": "kiṃsu tassa vicāraṇaṃ;",
  "sn1.64:4.3": "Kissassu vippahānena,",
  "sn1.64:4.4": "nibbānaṃ iti vuccatī”ti.",
  "sn1.64:5.1": "“<span class=\\\"var\\\" title=\\\"nandi saṃyojano (bj) | nandisaññojano (s1-3, km) | nandisaṃyojano (pts2)\\\" id=\\\"note96\\\">Nandīsaṃyojano</span> loko,",
  "sn1.64:5.2": "vitakkassa vicāraṇaṃ;",
  "sn1.64:5.3": "Taṇhāya vippahānena,",
  "sn1.64:5.4": "nibbānaṃ iti vuccatī”ti."
}