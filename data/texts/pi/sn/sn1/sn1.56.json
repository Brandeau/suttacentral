  "sn1.56:1.1": "Saṃyutta Nikāya 1",
  "sn1.56:2.1": "6. Jarāvagga",
  "sn1.56:3.1": "<span class=\\\"var\\\" title=\\\"dutiyajanetisuttaṃ (bj)\\\" id=\\\"note89\\\">56. Dutiyajanasutta</span>",
  "sn1.56:4.1": "“Kiṃsu janeti purisaṃ,",
  "sn1.56:4.2": "kiṃsu tassa vidhāvati;",
  "sn1.56:4.3": "Kiṃsu saṃsāramāpādi,",
  "sn1.56:4.4": "kismā na parimuccatī”ti.",
  "sn1.56:5.1": "“Taṇhā janeti purisaṃ,",
  "sn1.56:5.2": "cittamassa vidhāvati;",
  "sn1.56:5.3": "Satto saṃsāramāpādi,",
  "sn1.56:5.4": "dukkhā na parimuccatī”ti."
}