  "sn1.61:2.1": "<span class=\\\"var\\\" title=\\\"anvavaggo (bj) | andhavaggo (s1-3, pts2)\\\" id=\\\"note93\\\">7. Addhavagga</span>",
  "sn1.61:3.1": "61. Nāmasutta",
