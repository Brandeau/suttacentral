  "sn1.65:1.1": "Saṃyutta Nikāya 1",
  "sn1.65:2.1": "7. Addhavagga",
  "sn1.65:3.1": "65. Bandhanasutta",
  "sn1.65:4.1": "“Kiṃsu sambandhano loko,",
  "sn1.65:4.2": "kiṃsu tassa vicāraṇaṃ;",
  "sn1.65:4.3": "Kissassu vippahānena,",
  "sn1.65:4.4": "sabbaṃ chindati bandhanan”ti.",
  "sn1.65:5.1": "“Nandīsambandhano loko,",
  "sn1.65:5.2": "vitakkassa vicāraṇaṃ;",
  "sn1.65:5.3": "Taṇhāya vippahānena,",
  "sn1.65:5.4": "sabbaṃ chindati bandhanan”ti."
}