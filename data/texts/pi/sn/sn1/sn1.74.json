  "sn1.74:1.1": "Saṃyutta Nikāya 1",
  "sn1.74:2.1": "8. Chetvāvagga",
  "sn1.74:3.1": "74. Vuṭṭhisutta",
  "sn1.74:4.1": "“Kiṃsu uppatataṃ seṭṭhaṃ,",
  "sn1.74:4.2": "kiṃsu nipatataṃ varaṃ;",
  "sn1.74:4.3": "Kiṃsu pavajamānānaṃ,",
  "sn1.74:4.4": "kiṃsu pavadataṃ varan”ti.",
  "sn1.74:5.1": "“Bījaṃ uppatataṃ seṭṭhaṃ,",
  "sn1.74:5.2": "vuṭṭhi nipatataṃ varā;",
  "sn1.74:5.3": "Gāvo pavajamānānaṃ,",
  "sn1.74:5.4": "putto pavadataṃ varo”ti.",
  "sn1.74:6.1": "“Vijjā uppatataṃ seṭṭhā,",
  "sn1.74:6.2": "avijjā nipatataṃ varā;",
  "sn1.74:6.3": "Saṅgho pavajamānānaṃ,",
  "sn1.74:6.4": "buddho pavadataṃ varo”ti."
}