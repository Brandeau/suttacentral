  "sn1.10:1.1": "Saṃyutta Nikāya 1",
  "sn1.10:2.1": "1. Naḷavagga",
  "sn1.10:3.1": "10. Araññasutta",
  "sn1.10:4.1": "Sāvatthinidānaṃ.",
  "sn1.10:4.2": "Ekamantaṃ ṭhitā kho sā devatā bhagavantaṃ gāthāya ajjhabhāsi:",
  "sn1.10:5.1": "“Araññe viharantānaṃ,",
  "sn1.10:5.2": "santānaṃ brahmacārinaṃ;",
  "sn1.10:5.3": "Ekabhattaṃ bhuñjamānānaṃ,",
  "sn1.10:5.4": "kena vaṇṇo pasīdatī”ti.",
  "sn1.10:6.1": "“Atītaṃ nānusocanti,",
  "sn1.10:6.2": "nappajappanti nāgataṃ;",
  "sn1.10:6.3": "Paccuppannena yāpenti,",
  "sn1.10:6.4": "tena vaṇṇo pasīdati.",
  "sn1.10:7.1": "Anāgatappajappāya,",
  "sn1.10:7.2": "atītassānusocanā;",
  "sn1.10:7.3": "Etena bālā sussanti,",
  "sn1.10:7.4": "naḷova harito luto”ti.",
  "sn1.10:8.1": "Naḷavaggo paṭhamo.",
  "sn1.10:9.1": "Oghaṃ nimokkhaṃ upaneyyaṃ,",
  "sn1.10:9.2": "Accenti katichindi ca;",
  "sn1.10:9.3": "Jāgaraṃ appaṭividitā,",
  "sn1.10:9.4": "Susammuṭṭhā mānakāminā;",
  "sn1.10:9.5": "Araññe dasamo vutto,",
  "sn1.10:9.6": "Vaggo tena pavuccati."
}