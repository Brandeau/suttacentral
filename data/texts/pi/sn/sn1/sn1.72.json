  "sn1.72:1.1": "Saṃyutta Nikāya 1",
  "sn1.72:2.1": "8. Chetvāvagga",
  "sn1.72:3.1": "72. Rathasutta",
  "sn1.72:4.1": "“Kiṃsu rathassa paññāṇaṃ,",
  "sn1.72:4.2": "kiṃsu paññāṇamaggino;",
  "sn1.72:4.3": "Kiṃsu raṭṭhassa paññāṇaṃ,",
  "sn1.72:4.4": "kiṃsu paññāṇamitthiyā”ti.",
  "sn1.72:5.1": "<span class=\\\"cross\\\" title=\\\"^ja547#189 [Vessantarajātaka/ Dānakaṇḍa] | ^cnd7#36 [Pārāyanānugītigāthāniddesa]\\\" id=\\\"note105\\\">*</span> <a class=\\\"sc\\\" id=\\\"2\\\"></a><a class=\\\"pts1ed\\\" id=\\\"pts1ed1.42\\\"></a><a class=\\\"pts2ed\\\" id=\\\"pts2ed1.94\\\"></a>“Dhajo rathassa paññāṇaṃ,",
  "sn1.72:5.2": "dhūmo paññāṇamaggino;",
  "sn1.72:5.3": "Rājā raṭṭhassa paññāṇaṃ,",
  "sn1.72:5.4": "bhattā paññāṇamitthiyā”ti."
}