  "sn1.52:1.1": "Saṃyutta Nikāya 1",
  "sn1.52:2.1": "6. Jarāvagga",
  "sn1.52:3.1": "52. Ajarasāsutta",
  "sn1.52:4.1": "“Kiṃsu ajarasā sādhu,",
  "sn1.52:4.2": "kiṃsu sādhu adhiṭṭhitaṃ;",
  "sn1.52:4.3": "Kiṃsu narānaṃ ratanaṃ,",
  "sn1.52:4.4": "kiṃsu corehyahāriyan”ti.",
  "sn1.52:5.1": "“Sīlaṃ ajarasā sādhu,",
  "sn1.52:5.2": "saddhā sādhu adhiṭṭhitā;",
  "sn1.52:5.3": "Paññā narānaṃ ratanaṃ,",
  "sn1.52:5.4": "puññaṃ corehyahāriyan”ti."
}