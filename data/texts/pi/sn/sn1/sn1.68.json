  "sn1.68:1.1": "Saṃyutta Nikāya 1",
  "sn1.68:2.1": "7. Addhavagga",
  "sn1.68:3.1": "68. Pihitasutta",
  "sn1.68:4.1": "“Kenassu pihito loko,",
  "sn1.68:4.2": "kismiṃ loko patiṭṭhito;",
  "sn1.68:4.3": "Kenassu uḍḍito loko,",
  "sn1.68:4.4": "kenassu parivārito”ti.",
  "sn1.68:5.1": "“Maccunā pihito loko,",
  "sn1.68:5.2": "dukkhe loko patiṭṭhito;",
  "sn1.68:5.3": "Taṇhāya uḍḍito loko,",
  "sn1.68:5.4": "jarāya parivārito”ti."
}