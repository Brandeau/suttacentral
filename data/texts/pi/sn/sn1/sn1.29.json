  "sn1.29:2.1": "3. Sattivagga",
  "sn1.29:3.1": "29. Catucakkasutta",
  "sn1.29:4.1": "<span class=\\\"cross\\\" title=\\\"^sn2.28#1 [28. Nandivisālasutta] | ^pe5#6 [Hāravibhaṅgapañcamabhūmi]\\\" id=\\\"note49\\\">*</span> <a class=\\\"sc\\\" id=\\\"1\\\"></a><a class=\\\"pts1ed\\\" id=\\\"pts1ed1.16\\\"></a>“Catucakkaṃ navadvāraṃ,",
  "sn1.29:4.2": "puṇṇaṃ lobhena saṃyutaṃ;",
  "sn1.29:4.3": "Paṅkajātaṃ mahāvīra,",
  "sn1.29:4.4": "kathaṃ yātrā bhavissatī”ti.",
  "sn1.29:5.1": "“Chetvā naddhiṃ varattañca,",
  "sn1.29:5.2": "icchā lobhañca pāpakaṃ;",
  "sn1.29:5.3": "Samūlaṃ taṇhamabbuyha,",
  "sn1.29:5.4": "evaṃ yātrā bhavissatī”ti."
