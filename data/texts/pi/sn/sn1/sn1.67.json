  "sn1.67:1.1": "Saṃyutta Nikāya 1",
  "sn1.67:2.1": "7. Addhavagga",
  "sn1.67:3.1": "<span class=\\\"var\\\" title=\\\"uddito (pts1)\\\" id=\\\"note100\\\">67. Uḍḍitasutta</span>",
  "sn1.67:4.1": "“Kenassu uḍḍito loko,",
  "sn1.67:4.2": "kenassu parivārito;",
  "sn1.67:4.3": "Kenassu pihito loko,",
  "sn1.67:4.4": "kismiṃ loko patiṭṭhito”ti.",
  "sn1.67:5.1": "“Taṇhāya uḍḍito loko,",
  "sn1.67:5.2": "jarāya parivārito;",
  "sn1.67:5.3": "Maccunā pihito loko,",
  "sn1.67:5.4": "dukkhe loko patiṭṭhito”ti."
}