  "sn1.6:5.4": "<span class=\\\"var\\\" title=\\\"katīhi (bj, pts1-2)\\\" id=\\\"note5\\\">katibhi</span> parisujjhatī”ti.",
  "sn1.6:6.1": "“Pañca jāgarataṃ suttā,",
  "sn1.6:6.2": "pañca suttesu jāgarā;",
  "sn1.6:6.3": "<span class=\\\"var\\\" title=\\\"pañcahi (bj, pts1-2)\\\" id=\\\"note6\\\">Pañcabhi</span> rajamādeti,",
  "sn1.6:6.4": "<span class=\\\"var\\\" title=\\\"pañcahi (bj, pts1-2)\\\" id=\\\"note7\\\">pañcabhi</span> parisujjhatī”ti."
