  "sn1.60:1.1": "Saṃyutta Nikāya 1",
  "sn1.60:2.1": "6. Jarāvagga",
  "sn1.60:3.1": "60. Kavisutta",
  "sn1.60:4.1": "“Kiṃsu nidānaṃ gāthānaṃ,",
  "sn1.60:4.2": "kiṃsu tāsaṃ viyañjanaṃ;",
  "sn1.60:4.3": "Kiṃsu sannissitā gāthā,",
  "sn1.60:4.4": "kiṃsu gāthānamāsayo”ti.",
  "sn1.60:5.1": "“Chando nidānaṃ gāthānaṃ,",
  "sn1.60:5.2": "akkharā tāsaṃ viyañjanaṃ;",
  "sn1.60:5.3": "Nāmasannissitā gāthā,",
  "sn1.60:5.4": "kavi gāthānamāsayo”ti.",
  "sn1.60:6.1": "Jarāvaggo chaṭṭho.",
  "sn1.60:7.1": "Jarā ajarasā mittaṃ,",
  "sn1.60:7.2": "vatthu tīṇi janāni ca;",
  "sn1.60:7.3": "Uppatho ca dutiyo ca,",
  "sn1.60:7.4": "kavinā pūrito vaggoti."
}