  "sn43.14-43:1.1": "Saṃyutta Nikāya 43",
  "sn43.14-43:2.1": "2. Dutiyavagga",
  "sn43.14-43:3.1": "14–​43. Anāsavādisutta",
  "sn43.14-43:4.1": "“Anāsavañca vo, bhikkhave, desessāmi anāsavagāmiñca maggaṃ.",
  "sn43.14-43:4.2": "Taṃ suṇātha.",
  "sn43.14-43:4.3": "Katamañca, bhikkhave, anāsavaṃ … pe ….",
  "sn43.14-43:4.4": "<span class=\\\"brnum\\\">(3)</span>",
  "sn43.14-43:5.1": "Saccañca vo, bhikkhave, desessāmi saccagāmiñca maggaṃ.",
  "sn43.14-43:5.2": "Taṃ suṇātha.",
  "sn43.14-43:5.3": "Katamañca, bhikkhave, saccaṃ … pe ….",
  "sn43.14-43:5.4": "<span class=\\\"brnum\\\">(4)</span>",
  "sn43.14-43:6.1": "Pārañca vo, bhikkhave, desessāmi pāragāmiñca maggaṃ.",
  "sn43.14-43:6.2": "Taṃ suṇātha.",
  "sn43.14-43:6.3": "Katamañca, bhikkhave, pāraṃ … pe ….",
  "sn43.14-43:6.4": "<span class=\\\"brnum\\\">(5)</span>",
  "sn43.14-43:7.1": "Nipuṇañca vo, bhikkhave, desessāmi nipuṇagāmiñca maggaṃ.",
  "sn43.14-43:7.2": "Taṃ suṇātha.",
  "sn43.14-43:7.3": "Katamañca, bhikkhave, nipuṇaṃ … pe ….",
  "sn43.14-43:7.4": "<span class=\\\"brnum\\\">(6)</span>",
  "sn43.14-43:8.1": "Sududdasañca vo, bhikkhave, desessāmi sududdasagāmiñca maggaṃ.",
  "sn43.14-43:8.2": "Taṃ suṇātha.",
  "sn43.14-43:8.3": "Katamañca, bhikkhave, sududdasaṃ … pe ….",
  "sn43.14-43:8.4": "<span class=\\\"brnum\\\">(7)</span>",
  "sn43.14-43:9.1": "Ajajjarañca vo, bhikkhave, desessāmi ajajjaragāmiñca maggaṃ.",
  "sn43.14-43:9.2": "Taṃ suṇātha.",
  "sn43.14-43:9.3": "Katamañca, bhikkhave, ajajjaraṃ … pe ….",
  "sn43.14-43:9.4": "<span class=\\\"brnum\\\">(8)</span>",
  "sn43.14-43:10.1": "Dhuvañca vo, bhikkhave, desessāmi dhuvagāmiñca maggaṃ.",
  "sn43.14-43:10.2": "Taṃ suṇātha.",
  "sn43.14-43:10.3": "Katamañca, bhikkhave, dhuvaṃ … pe ….",
  "sn43.14-43:10.4": "<span class=\\\"brnum\\\">(9)</span>",
  "sn43.14-43:11.1": "Apalokitañca vo, bhikkhave, desessāmi apalokitagāmiñca maggaṃ.",
  "sn43.14-43:11.2": "Taṃ suṇātha.",
  "sn43.14-43:11.3": "Katamañca, bhikkhave, apalokitaṃ … pe ….",
  "sn43.14-43:11.4": "<span class=\\\"brnum\\\">(10)</span>",
  "sn43.14-43:12.1": "Anidassanañca vo, bhikkhave, desessāmi anidassanagāmiñca maggaṃ.",
  "sn43.14-43:12.2": "Taṃ suṇātha.",
  "sn43.14-43:12.3": "Katamañca, bhikkhave, anidassanaṃ … pe ….",
  "sn43.14-43:12.4": "<span class=\\\"brnum\\\">(11)</span>",
  "sn43.14-43:13.1": "Nippapañcañca vo, bhikkhave, desessāmi nippapañcagāmiñca maggaṃ.",
  "sn43.14-43:13.2": "Taṃ suṇātha.",
  "sn43.14-43:13.3": "Katamañca, bhikkhave, nippapañcaṃ … pe … ?",
  "sn43.14-43:13.4": "<span class=\\\"brnum\\\">(12)</span>",
  "sn43.14-43:14.1": "Santañca vo, bhikkhave, desessāmi santagāmiñca maggaṃ.",
  "sn43.14-43:14.2": "Taṃ suṇātha.",
  "sn43.14-43:14.3": "Katamañca, bhikkhave, santaṃ … pe ….",
  "sn43.14-43:14.4": "<span class=\\\"brnum\\\">(13)</span>",
  "sn43.14-43:15.1": "Amatañca vo, bhikkhave, desessāmi amatagāmiñca maggaṃ.",
  "sn43.14-43:15.2": "Taṃ suṇātha.",
  "sn43.14-43:15.3": "Katamañca, bhikkhave, amataṃ … pe ….",
  "sn43.14-43:15.4": "<span class=\\\"brnum\\\">(14)</span>",
  "sn43.14-43:16.1": "Paṇītañca vo, bhikkhave, desessāmi paṇītagāmiñca maggaṃ.",
  "sn43.14-43:16.2": "Taṃ suṇātha.",
  "sn43.14-43:16.3": "Katamañca, bhikkhave, paṇītaṃ … pe ….",
  "sn43.14-43:16.4": "<span class=\\\"brnum\\\">(15)</span>",
  "sn43.14-43:17.1": "Sivañca vo, bhikkhave, desessāmi sivagāmiñca maggaṃ.",
  "sn43.14-43:17.2": "Taṃ suṇātha.",
  "sn43.14-43:17.3": "Katamañca, bhikkhave, sivaṃ … pe ….",
  "sn43.14-43:17.4": "<span class=\\\"brnum\\\">(16)</span>",
  "sn43.14-43:18.1": "Khemañca vo, bhikkhave, desessāmi khemagāmiñca maggaṃ.",
  "sn43.14-43:18.2": "Taṃ suṇātha.",
  "sn43.14-43:18.3": "Katamañca, bhikkhave, khemaṃ … pe ….",
  "sn43.14-43:18.4": "<span class=\\\"brnum\\\">(17)</span>",
  "sn43.14-43:19.1": "Taṇhākkhayañca vo, bhikkhave, desessāmi taṇhākkhayagāmiñca maggaṃ.",
  "sn43.14-43:19.2": "Taṃ suṇātha.",
  "sn43.14-43:19.3": "Katamañca, bhikkhave, taṇhākkhayaṃ … pe … ?",
  "sn43.14-43:19.4": "<span class=\\\"brnum\\\">(18)</span>",
  "sn43.14-43:20.1": "Acchariyañca vo, bhikkhave, desessāmi acchariyagāmiñca maggaṃ.",
  "sn43.14-43:20.2": "Taṃ suṇātha.",
  "sn43.14-43:20.3": "Katamañca, bhikkhave, acchariyaṃ … pe ….",
  "sn43.14-43:20.4": "<span class=\\\"brnum\\\">(19)</span>",
  "sn43.14-43:21.1": "Abbhutañca vo, bhikkhave, desessāmi abbhutagāmiñca maggaṃ.",
  "sn43.14-43:21.2": "Taṃ suṇātha.",
  "sn43.14-43:21.3": "Katamañca, bhikkhave, abbhutaṃ … pe ….",
  "sn43.14-43:21.4": "<span class=\\\"brnum\\\">(20)</span>",
  "sn43.14-43:22.1": "Anītikañca vo, bhikkhave, desessāmi anītikagāmiñca maggaṃ.",
  "sn43.14-43:22.2": "Taṃ suṇātha.",
  "sn43.14-43:22.3": "Katamañca, bhikkhave, anītikaṃ … pe ….",
  "sn43.14-43:22.4": "<span class=\\\"brnum\\\">(21)</span>",
  "sn43.14-43:23.1": "Anītikadhammañca vo, bhikkhave, desessāmi anītikadhammagāmiñca maggaṃ.",
  "sn43.14-43:23.2": "Taṃ suṇātha.",
  "sn43.14-43:23.3": "Katamañca, bhikkhave, anītikadhammaṃ … pe ….",
  "sn43.14-43:23.4": "<span class=\\\"brnum\\\">(22)</span>",
  "sn43.14-43:24.1": "Nibbānañca vo, bhikkhave, desessāmi nibbānagāmiñca maggaṃ.",
  "sn43.14-43:24.2": "Taṃ suṇātha.",
  "sn43.14-43:24.3": "Katamañca, bhikkhave, nibbānaṃ … pe ….",
  "sn43.14-43:24.4": "<span class=\\\"brnum\\\">(23)</span>",
  "sn43.14-43:25.1": "<span class=\\\"var\\\" title=\\\"abyāpajjhañca (bj, s1-3, km) | avyāpajjhañca (pts1)\\\" id=\\\"note527\\\">Abyābajjhañca</span> vo, bhikkhave, desessāmi abyābajjhagāmiñca maggaṃ.",
  "sn43.14-43:25.2": "Taṃ suṇātha.",
  "sn43.14-43:25.3": "Katamañca, bhikkhave, abyābajjhaṃ … pe ….",
  "sn43.14-43:25.4": "<span class=\\\"brnum\\\">(24)</span>",
  "sn43.14-43:26.1": "Virāgañca vo, bhikkhave, desessāmi virāgagāmiñca maggaṃ.",
  "sn43.14-43:26.2": "Taṃ suṇātha.",
  "sn43.14-43:26.3": "Katamo ca, bhikkhave, virāgo … pe … ?",
  "sn43.14-43:26.4": "<span class=\\\"brnum\\\">(25)</span>",
  "sn43.14-43:27.1": "Suddhiñca vo, bhikkhave, desessāmi suddhigāmiñca maggaṃ.",
  "sn43.14-43:27.2": "Taṃ suṇātha.",
  "sn43.14-43:27.3": "Katamā ca, bhikkhave, suddhi … pe ….",
  "sn43.14-43:27.4": "<span class=\\\"brnum\\\">(26)</span>",
  "sn43.14-43:28.1": "Muttiñca vo, bhikkhave, desessāmi muttigāmiñca maggaṃ.",
  "sn43.14-43:28.2": "Taṃ suṇātha.",
  "sn43.14-43:28.3": "Katamā ca, bhikkhave, mutti … pe ….",
  "sn43.14-43:28.4": "<span class=\\\"brnum\\\">(27)</span>",
  "sn43.14-43:29.1": "Anālayañca vo, bhikkhave, desessāmi anālayagāmiñca maggaṃ.",
  "sn43.14-43:29.2": "Taṃ suṇātha.",
  "sn43.14-43:29.3": "Katamo ca, bhikkhave, anālayo … pe ….",
  "sn43.14-43:29.4": "<span class=\\\"brnum\\\">(28)</span>",
  "sn43.14-43:30.1": "Dīpañca vo, bhikkhave, desessāmi dīpagāmiñca maggaṃ.",
  "sn43.14-43:30.2": "Taṃ suṇātha.",
  "sn43.14-43:30.3": "Katamañca, bhikkhave, dīpaṃ … pe ….",
  "sn43.14-43:30.4": "<span class=\\\"brnum\\\">(29)</span>",
  "sn43.14-43:31.1": "Leṇañca vo, bhikkhave, desessāmi leṇagāmiñca maggaṃ.",
  "sn43.14-43:31.2": "Taṃ suṇātha.",
  "sn43.14-43:31.3": "Katamañca, bhikkhave, leṇaṃ … pe ….",
  "sn43.14-43:31.4": "<span class=\\\"brnum\\\">(30)</span>",
  "sn43.14-43:32.1": "Tāṇañca vo, bhikkhave, desessāmi tāṇagāmiñca maggaṃ.",
  "sn43.14-43:32.2": "Taṃ suṇātha.",
  "sn43.14-43:32.3": "Katamañca, bhikkhave, tāṇaṃ … pe ….",
  "sn43.14-43:32.4": "<span class=\\\"brnum\\\">(31)</span>",
  "sn43.14-43:33.1": "Saraṇañca vo, bhikkhave, desessāmi saraṇagāmiñca maggaṃ.",
  "sn43.14-43:33.2": "Taṃ suṇātha.",
  "sn43.14-43:33.3": "Katamañca, bhikkhave, saraṇaṃ … pe ….",
  "sn43.14-43:33.4": "<span class=\\\"brnum\\\">(32)</span>",
  "sn43.14-43:34.1": "Bāttiṃsatimaṃ."
}