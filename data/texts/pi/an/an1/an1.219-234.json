  "_markup_path": "markup/an/an01/an1.219-234.html",
  "_alt_strings": [
    "en/sujato/an/an01/an1.219-234.json"
  ],
  "an1.219-234:1.1": "Aṅguttara Nikāya 1",
  "an1.219-234:2.1": "17. Catutthavagga",
  "an1.219-234:3.1": "219–​234",
  "an1.219-234:4.1": "“Etadaggaṃ, bhikkhave, mama sāvakānaṃ bhikkhūnaṃ bahussutānaṃ yadidaṃ ānando.",
  "an1.219-234:4.2": "<span class=\"brnum\">(1)</span>",
  "an1.219-234:5.1": "… Satimantānaṃ yadidaṃ ānando.",
  "an1.219-234:5.2": "<span class=\"brnum\">(2)</span>",
  "an1.219-234:6.1": "… Gatimantānaṃ yadidaṃ ānando.",
  "an1.219-234:6.2": "<span class=\"brnum\">(3)</span>",
  "an1.219-234:7.1": "… Dhitimantānaṃ yadidaṃ ānando.",
  "an1.219-234:7.2": "<span class=\"brnum\">(4)</span>",
  "an1.219-234:8.1": "… Upaṭṭhākānaṃ yadidaṃ ānando.",
  "an1.219-234:8.2": "<span class=\"brnum\">(5)</span>",
  "an1.219-234:9.1": "… Mahāparisānaṃ yadidaṃ uruvelakassapo.",
  "an1.219-234:9.2": "<span class=\"brnum\">(6)</span>",
  "an1.219-234:10.1": "… Kulappasādakānaṃ yadidaṃ kāḷudāyī.",
  "an1.219-234:10.2": "<span class=\"brnum\">(7)</span>",
  "an1.219-234:11.1": "<span class=\"cross\" title=\"^mi5.5.3#1 [Buddhaappābādhapañha] buddhaappābādhapañhe\" id=\"note38\">*</span> <a class=\"sc\" id=\"8\"></a>… Appābādhānaṃ yadidaṃ <span class=\"var\" title=\"bakkulo (bj, s1-3, km, pts1)\" id=\"note39\">bākulo</span>.",
  "an1.219-234:11.2": "<span class=\"brnum\">(8)</span>",
  "an1.219-234:12.1": "… Pubbenivāsaṃ anussarantānaṃ yadidaṃ sobhito.",
  "an1.219-234:12.2": "<span class=\"brnum\">(9)</span>",
  "an1.219-234:13.1": "… Vinayadharānaṃ yadidaṃ upāli.",
  "an1.219-234:13.2": "<span class=\"brnum\">(10)</span>",
  "an1.219-234:14.1": "… Bhikkhunovādakānaṃ yadidaṃ nandako.",
  "an1.219-234:14.2": "<span class=\"brnum\">(11)</span>",
  "an1.219-234:15.1": "… Indriyesu guttadvārānaṃ yadidaṃ nando.",
  "an1.219-234:15.2": "<span class=\"brnum\">(12)</span>",
  "an1.219-234:16.1": "… Bhikkhuovādakānaṃ yadidaṃ mahākappino.",
  "an1.219-234:16.2": "<span class=\"brnum\">(13)</span>",
  "an1.219-234:17.1": "… Tejodhātukusalānaṃ yadidaṃ sāgato.",
  "an1.219-234:17.2": "<span class=\"brnum\">(14)</span>",
  "an1.219-234:18.1": "… Paṭibhāneyyakānaṃ yadidaṃ rādho.",
  "an1.219-234:18.2": "<span class=\"brnum\">(15)</span>",
  "an1.219-234:19.1": "… Lūkhacīvaradharānaṃ yadidaṃ mogharājā”ti.",
  "an1.219-234:19.2": "<span class=\"brnum\">(16)</span>",
  "an1.219-234:20.1": "Vaggo catuttho."
}