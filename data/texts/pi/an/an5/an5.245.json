  "an5.245:1.1": "Aṅguttara Nikāya 5",
  "an5.245:2.1": "25. Duccaritavagga",
  "an5.245:3.1": "245. Dutiyaduccaritasutta",
  "an5.245:4.1": "“Pañcime, bhikkhave, ādīnavā duccarite.",
  "an5.245:4.2": "Katame pañca?",
  "an5.245:4.3": "Attāpi attānaṃ upavadati;",
  "an5.245:4.4": "anuvicca viññū garahanti;",
  "an5.245:4.5": "pāpako kittisaddo abbhuggacchati;",
  "an5.245:4.6": "saddhammā vuṭṭhāti;",
  "an5.245:4.7": "asaddhamme patiṭṭhāti.",
  "an5.245:4.8": "Ime kho, bhikkhave, pañca ādīnavā duccarite.",
  "an5.245:5.1": "Pañcime, bhikkhave, ānisaṃsā sucarite.",
  "an5.245:5.2": "Katame pañca?",
  "an5.245:5.3": "Attāpi attānaṃ na upavadati;",
  "an5.245:5.4": "anuvicca viññū pasaṃsanti;",
  "an5.245:5.5": "kalyāṇo kittisaddo abbhuggacchati;",
  "an5.245:5.6": "asaddhammā vuṭṭhāti;",
  "an5.245:5.7": "saddhamme patiṭṭhāti.",
  "an5.245:5.8": "Ime kho, bhikkhave, pañca ānisaṃsā sucarite”ti.",
  "an5.245:6.1": "Pañcamaṃ."
}