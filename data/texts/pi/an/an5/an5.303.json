  "an5.303:1.1": "Aṅguttara Nikāya 5",
  "an5.303:2.1": "<span class=\"var\" id=\"note564\" title=\"rāgādipeyyālaṃ (bj)\">29. Rāgapeyyāla</span>",
  "an5.303:3.1": "303",
  "an5.303:4.1": "“Rāgassa, bhikkhave, abhiññāya pañca dhammā bhāvetabbā.",
  "an5.303:4.2": "Katame pañca?",
  "an5.303:4.3": "Asubhasaññā, maraṇasaññā, ādīnavasaññā, āhāre paṭikūlasaññā, sabbaloke <span class=\"var\" title=\"sabbatthapi evameva\" id=\"note565\">anabhiratasaññā</span>—",
  "an5.303:4.4": "rāgassa, bhikkhave, abhiññāya ime pañca dhammā bhāvetabbā”ti.",
  "an5.303:4.5": "<span class=\"brnum\">(1)</span>"
}