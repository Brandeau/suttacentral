  "an5.93:1.1": "Aṅguttara Nikāya 5",
  "an5.93:2.1": "10. Kakudhavagga",
  "an5.93:3.1": "<span class=\"var\" id=\"note256\" title=\"aññābyākaraṇasuttaṃ (bj)\">93. Byākaraṇasutta</span>",
  "an5.93:4.1": "“Pañcimāni, bhikkhave, aññābyākaraṇāni.",
  "an5.93:4.2": "Katamāni pañca?",
  "an5.93:4.3": "Mandattā momūhattā aññaṃ byākaroti;",
  "an5.93:4.4": "pāpiccho icchāpakato aññaṃ byākaroti;",
  "an5.93:4.5": "ummādā cittakkhepā aññaṃ byākaroti;",
  "an5.93:4.6": "adhimānena aññaṃ byākaroti;",
  "an5.93:4.7": "sammadeva aññaṃ byākaroti.",
  "an5.93:4.8": "Imāni kho, bhikkhave, pañca aññābyākaraṇānī”ti.",
  "an5.93:5.1": "Tatiyaṃ."
}