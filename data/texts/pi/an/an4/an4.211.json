  "an4.211:1.1": "Aṅguttara Nikāya 4",
  "an4.211:2.1": "<span class=\"var\" id=\"note539\" title=\"parisasobhanavaggo (bj)\">22. Parisāvagga</span>",
  "an4.211:3.1": "<span class=\"var\" id=\"note540\" title=\"parisasobhanasuttaṃ (bj)\">211. Parisāsutta</span>",
  "an4.211:4.1": "“Cattārome, bhikkhave, parisadūsanā.",
  "an4.211:4.2": "Katame cattāro?",
  "an4.211:4.3": "<span class=\"var\" title=\"idha bhikkhave bhikkhu (pts1, mr)\" id=\"note541\">Bhikkhu, bhikkhave</span>, dussīlo pāpadhammo parisadūsano;",
  "an4.211:4.4": "bhikkhunī, bhikkhave, dussīlā pāpadhammā parisadūsanā;",
  "an4.211:4.5": "upāsako, bhikkhave, dussīlo pāpadhammo parisadūsano;",
  "an4.211:4.6": "upāsikā, bhikkhave, dussīlā pāpadhammā parisadūsanā.",
  "an4.211:4.7": "Ime kho, bhikkhave, cattāro parisadūsanā.",
  "an4.211:5.1": "Cattārome, bhikkhave, parisasobhanā.",
  "an4.211:5.2": "Katame cattāro?",
  "an4.211:5.3": "Bhikkhu, bhikkhave, sīlavā kalyāṇadhammo parisasobhano;",
  "an4.211:5.4": "bhikkhunī, bhikkhave, sīlavatī kalyāṇadhammā parisasobhanā;",
  "an4.211:5.5": "upāsako, bhikkhave, sīlavā kalyāṇadhammo parisasobhano;",
  "an4.211:5.6": "upāsikā, bhikkhave, sīlavatī kalyāṇadhammā parisasobhanā.",
  "an4.211:5.7": "Ime kho, bhikkhave, cattāro parisasobhanā”ti.",
  "an4.211:6.1": "Paṭhamaṃ."
}