  "an4.65:1.1": "Aṅguttara Nikāya 4",
  "an4.65:2.1": "7. Pattakammavagga",
  "an4.65:3.1": "<span class=\"var\" id=\"note212\" title=\"rūpa (ppamāṇa) suttaṃ (bj)\">65. Rūpasutta</span>",
  "an4.65:4.1": "“Cattārome, bhikkhave, puggalā santo saṃvijjamānā lokasmiṃ.",
  "an4.65:4.2": "Katame cattāro?",
  "an4.65:4.3": "Rūpappamāṇo rūpappasanno, ghosappamāṇo ghosappasanno, lūkhappamāṇo lūkhappasanno, dhammappamāṇo dhammappasanno—",
  "an4.65:4.4": "ime kho, bhikkhave, cattāro puggalā santo saṃvijjamānā lokasminti.",
  "an4.65:5.1": "Ye <span class=\"var\" title=\"ye ca rūpena pāmiṃsu (bj, s1-3, km, pts1)\" id=\"note213\">ca rūpe pamāṇiṃsu</span>,",
  "an4.65:5.2": "ye ca ghosena anvagū;",
  "an4.65:5.3": "Chandarāgavasūpetā,",
  "an4.65:5.4": "<span class=\"var\" title=\"na te jānanti taṃ janaṃ (bj, pts1) | na te jānanti tañjanaṃ (s1-3, km)\" id=\"note214\">nābhijānanti te janā</span>.",
  "an4.65:6.1": "Ajjhattañca na jānāti,",
  "an4.65:6.2": "bahiddhā ca na passati;",
  "an4.65:6.3": "Samantāvaraṇo bālo,",
  "an4.65:6.4": "sa ve ghosena vuyhati.",
  "an4.65:7.1": "Ajjhattañca na jānāti,",
  "an4.65:7.2": "bahiddhā ca vipassati;",
  "an4.65:7.3": "Bahiddhā phaladassāvī,",
  "an4.65:7.4": "sopi ghosena vuyhati.",
  "an4.65:8.1": "Ajjhattañca pajānāti,",
  "an4.65:8.2": "bahiddhā ca vipassati;",
  "an4.65:8.3": "Vinīvaraṇadassāvī,",
  "an4.65:8.4": "na so ghosena vuyhatī”ti.",
  "an4.65:9.1": "Pañcamaṃ."
}