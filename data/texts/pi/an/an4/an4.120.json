  "an4.120:1.1": "Aṅguttara Nikāya 4",
  "an4.120:2.1": "12. Kesivagga",
  "an4.120:3.1": "120. Dutiyabhayasutta",
  "an4.120:4.1": "“Cattārimāni, bhikkhave, bhayāni.",
  "an4.120:4.2": "Katamāni cattāri?",
  "an4.120:4.3": "Aggibhayaṃ, udakabhayaṃ, rājabhayaṃ, corabhayaṃ—",
  "an4.120:4.4": "imāni kho, bhikkhave, cattāri bhayānī”ti.",
  "an4.120:5.1": "Dasamaṃ.",
  "an4.120:6.1": "Kesivaggo dutiyo.",
  "an4.120:7.1": "Kesi javo patodo ca,",
  "an4.120:7.2": "Nāgo ṭhānena pañcamaṃ;",
  "an4.120:7.3": "Appamādo ca ārakkho,",
  "an4.120:7.4": "Saṃvejanīyañca dve bhayāti."
}