  "an2.163-179:1.1": "Aṅguttara Nikāya 2",
  "an2.163-179:2.1": "15. Samāpattivagga",
  "an2.163-179:3.1": "163",
  "an2.163-179:4.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:4.2": "Katame dve?",
  "an2.163-179:4.3": "Samāpattikusalatā ca samāpattivuṭṭhānakusalatā ca.",
  "an2.163-179:4.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.163-179:5.1": "164",
  "an2.163-179:6.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:6.2": "Katame dve?",
  "an2.163-179:6.3": "Ajjavañca maddavañca.",
  "an2.163-179:6.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:7.1": "165",
  "an2.163-179:8.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:8.2": "Katame dve?",
  "an2.163-179:8.3": "Khanti ca soraccañca.",
  "an2.163-179:8.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:9.1": "166",
  "an2.163-179:10.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:10.2": "Katame dve?",
  "an2.163-179:10.3": "Sākhalyañca paṭisanthāro ca.",
  "an2.163-179:10.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:11.1": "167",
  "an2.163-179:12.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:12.2": "Katame dve?",
  "an2.163-179:12.3": "Avihiṃsā ca soceyyañca.",
  "an2.163-179:12.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:13.1": "168",
  "an2.163-179:14.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:14.2": "Katame dve?",
  "an2.163-179:14.3": "Indriyesu aguttadvāratā ca bhojane amattaññutā ca.",
  "an2.163-179:14.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:15.1": "169",
  "an2.163-179:16.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:16.2": "Katame dve?",
  "an2.163-179:16.3": "Indriyesu guttadvāratā ca bhojane mattaññutā ca.",
  "an2.163-179:16.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:17.1": "170",
  "an2.163-179:18.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:18.2": "Katame dve?",
  "an2.163-179:18.3": "Paṭisaṅkhānabalañca bhāvanābalañca.",
  "an2.163-179:18.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:19.1": "171",
  "an2.163-179:20.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:20.2": "Katame dve?",
  "an2.163-179:20.3": "Satibalañca samādhibalañca.",
  "an2.163-179:20.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:21.1": "172",
  "an2.163-179:22.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:22.2": "Katame dve?",
  "an2.163-179:22.3": "Samatho ca vipassanā ca.",
  "an2.163-179:22.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:23.1": "173",
  "an2.163-179:24.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:24.2": "Katame dve?",
  "an2.163-179:24.3": "Sīlavipatti ca diṭṭhivipatti ca.",
  "an2.163-179:24.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:25.1": "174",
  "an2.163-179:26.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:26.2": "Katame dve?",
  "an2.163-179:26.3": "Sīlasampadā ca diṭṭhisampadā ca.",
  "an2.163-179:26.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:27.1": "175",
  "an2.163-179:28.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:28.2": "Katame dve?",
  "an2.163-179:28.3": "Sīlavisuddhi ca diṭṭhivisuddhi ca.",
  "an2.163-179:28.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:29.1": "176",
  "an2.163-179:30.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:30.2": "Katame dve?",
  "an2.163-179:30.3": "Diṭṭhivisuddhi ca yathādiṭṭhissa ca padhānaṃ.",
  "an2.163-179:30.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:31.1": "177",
  "an2.163-179:32.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:32.2": "Katame dve?",
  "an2.163-179:32.3": "Asantuṭṭhitā ca kusalesu dhammesu, appaṭivānitā ca padhānasmiṃ.",
  "an2.163-179:32.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:33.1": "178",
  "an2.163-179:34.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:34.2": "Katame dve?",
  "an2.163-179:34.3": "Muṭṭhassaccañca asampajaññañca.",
  "an2.163-179:34.4": "Ime kho, bhikkhave, dve dhammā”.",
  "an2.163-179:35.1": "179",
  "an2.163-179:36.1": "“Dveme, bhikkhave, dhammā.",
  "an2.163-179:36.2": "Katame dve?",
  "an2.163-179:36.3": "Sati ca sampajaññañca.",
  "an2.163-179:36.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.163-179:37.1": "Samāpattivaggo pañcamo.",
  "an2.163-179:38.1": "Tatiyo paṇṇāsako samatto."
}