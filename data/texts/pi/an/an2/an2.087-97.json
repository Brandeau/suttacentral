  "an2.87-97:1.1": "Aṅguttara Nikāya 2",
  "an2.87-97:2.1": "9. Dhammavagga",
  "an2.87-97:3.1": "87",
  "an2.87-97:4.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:4.2": "Katame dve?",
  "an2.87-97:4.3": "Cetovimutti ca paññāvimutti ca.",
  "an2.87-97:4.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:5.1": "88",
  "an2.87-97:6.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:6.2": "Katame dve?",
  "an2.87-97:6.3": "Paggāho ca avikkhepo ca.",
  "an2.87-97:6.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:7.1": "89",
  "an2.87-97:8.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:8.2": "Katame dve?",
  "an2.87-97:8.3": "Nāmañca rūpañca.",
  "an2.87-97:8.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:9.1": "90",
  "an2.87-97:10.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:10.2": "Katame dve?",
  "an2.87-97:10.3": "Vijjā ca vimutti ca.",
  "an2.87-97:10.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:11.1": "91",
  "an2.87-97:12.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:12.2": "Katame dve?",
  "an2.87-97:12.3": "Bhavadiṭṭhi ca vibhavadiṭṭhi ca.",
  "an2.87-97:12.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:13.1": "92",
  "an2.87-97:14.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:14.2": "Katame dve?",
  "an2.87-97:14.3": "Ahirikañca anottappañca.",
  "an2.87-97:14.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:15.1": "93",
  "an2.87-97:16.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:16.2": "Katame dve?",
  "an2.87-97:16.3": "Hirī ca ottappañca.",
  "an2.87-97:16.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:17.1": "94",
  "an2.87-97:18.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:18.2": "Katame dve?",
  "an2.87-97:18.3": "Dovacassatā ca pāpamittatā ca.",
  "an2.87-97:18.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:19.1": "95",
  "an2.87-97:20.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:20.2": "Katame dve?",
  "an2.87-97:20.3": "Sovacassatā ca kalyāṇamittatā ca.",
  "an2.87-97:20.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:21.1": "96",
  "an2.87-97:22.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:22.2": "Katame dve?",
  "an2.87-97:22.3": "Dhātukusalatā ca manasikārakusalatā ca.",
  "an2.87-97:22.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:23.1": "97",
  "an2.87-97:24.1": "“Dveme, bhikkhave, dhammā.",
  "an2.87-97:24.2": "Katame dve?",
  "an2.87-97:24.3": "Āpattikusalatā ca āpattivuṭṭhānakusalatā ca.",
  "an2.87-97:24.4": "Ime kho, bhikkhave, dve dhammā”ti.",
  "an2.87-97:25.1": "Dhammavaggo catuttho."
}