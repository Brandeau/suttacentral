  "an2.141-150:1.1": "Aṅguttara Nikāya 2",
  "an2.141-150:2.1": "13. Dānavagga",
  "an2.141-150:3.1": "141",
  "an2.141-150:4.1": "“Dvemāni, bhikkhave, dānāni.",
  "an2.141-150:4.2": "Katamāni dve?",
  "an2.141-150:4.3": "Āmisadānañca dhammadānañca.",
  "an2.141-150:4.4": "Imāni kho, bhikkhave, dve dānāni.",
  "an2.141-150:4.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ dānānaṃ yadidaṃ dhammadānan”ti.",
  "an2.141-150:5.1": "142",
  "an2.141-150:6.1": "“Dveme, bhikkhave, yāgā.",
  "an2.141-150:6.2": "Katame dve?",
  "an2.141-150:6.3": "Āmisayāgo ca dhammayāgo ca.",
  "an2.141-150:6.4": "Ime kho, bhikkhave, dve yāgā.",
  "an2.141-150:6.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ yāgānaṃ yadidaṃ dhammayāgo”ti.",
  "an2.141-150:7.1": "143",
  "an2.141-150:8.1": "“Dveme, bhikkhave, cāgā.",
  "an2.141-150:8.2": "Katame dve?",
  "an2.141-150:8.3": "Āmisacāgo ca dhammacāgo ca.",
  "an2.141-150:8.4": "Ime kho, bhikkhave, dve cāgā.",
  "an2.141-150:8.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ cāgānaṃ yadidaṃ dhammacāgo”ti.",
  "an2.141-150:9.1": "144",
  "an2.141-150:10.1": "“Dveme, bhikkhave, pariccāgā.",
  "an2.141-150:10.2": "Katame dve?",
  "an2.141-150:10.3": "Āmisapariccāgo ca dhammapariccāgo ca.",
  "an2.141-150:10.4": "Ime kho, bhikkhave, dve pariccāgā.",
  "an2.141-150:10.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ pariccāgānaṃ yadidaṃ dhammapariccāgo”ti.",
  "an2.141-150:11.1": "145",
  "an2.141-150:12.1": "“Dveme, bhikkhave, bhogā.",
  "an2.141-150:12.2": "Katame dve?",
  "an2.141-150:12.3": "Āmisabhogo ca dhammabhogo ca.",
  "an2.141-150:12.4": "Ime kho, bhikkhave, dve bhogā.",
  "an2.141-150:12.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ bhogānaṃ yadidaṃ dhammabhogo”ti.",
  "an2.141-150:13.1": "146",
  "an2.141-150:14.1": "“Dveme, bhikkhave, sambhogā.",
  "an2.141-150:14.2": "Katame dve?",
  "an2.141-150:14.3": "Āmisasambhogo ca dhammasambhogo ca.",
  "an2.141-150:14.4": "Ime kho, bhikkhave, dve sambhogā.",
  "an2.141-150:14.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sambhogānaṃ yadidaṃ dhammasambhogo”ti.",
  "an2.141-150:15.1": "147",
  "an2.141-150:16.1": "“Dveme, bhikkhave, saṃvibhāgā.",
  "an2.141-150:16.2": "Katame dve?",
  "an2.141-150:16.3": "Āmisasaṃvibhāgo ca dhammasaṃvibhāgo ca.",
  "an2.141-150:16.4": "Ime kho, bhikkhave, dve saṃvibhāgā.",
  "an2.141-150:16.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ saṃvibhāgānaṃ yadidaṃ dhammasaṃvibhāgo”ti.",
  "an2.141-150:17.1": "148",
  "an2.141-150:18.1": "“Dveme, bhikkhave, saṅgahā.",
  "an2.141-150:18.2": "Katame dve?",
  "an2.141-150:18.3": "Āmisasaṅgaho ca dhammasaṅgaho ca.",
  "an2.141-150:18.4": "Ime kho, bhikkhave, dve saṅgahā.",
  "an2.141-150:18.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ saṅgahānaṃ yadidaṃ dhammasaṅgaho”ti.",
  "an2.141-150:19.1": "149",
  "an2.141-150:20.1": "“Dveme, bhikkhave, anuggahā.",
  "an2.141-150:20.2": "Katame dve?",
  "an2.141-150:20.3": "Āmisānuggaho ca dhammānuggaho ca.",
  "an2.141-150:20.4": "Ime kho, bhikkhave, dve anuggahā.",
  "an2.141-150:20.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ anuggahānaṃ yadidaṃ dhammānuggaho”ti.",
  "an2.141-150:21.1": "150",
  "an2.141-150:22.1": "“Dvemā, bhikkhave, anukampā.",
  "an2.141-150:22.2": "Katamā dve?",
  "an2.141-150:22.3": "Āmisānukampā ca dhammānukampā ca.",
  "an2.141-150:22.4": "Imā kho, bhikkhave, dve anukampā.",
  "an2.141-150:22.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ anukampānaṃ yadidaṃ dhammānukampā”ti.",
  "an2.141-150:23.1": "Dānavaggo tatiyo."
}