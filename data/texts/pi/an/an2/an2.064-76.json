  "an2.64-76:1.1": "Aṅguttara Nikāya 2",
  "an2.64-76:2.1": "7. Sukhavagga",
  "an2.64-76:3.1": "64",
  "an2.64-76:4.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:4.2": "Katamāni dve?",
  "an2.64-76:4.3": "Gihisukhañca pabbajitasukhañca.",
  "an2.64-76:4.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:4.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ pabbajitasukhan”ti.",
  "an2.64-76:5.1": "65",
  "an2.64-76:6.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:6.2": "Katamāni dve?",
  "an2.64-76:6.3": "Kāmasukhañca nekkhammasukhañca.",
  "an2.64-76:6.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:6.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nekkhammasukhan”ti.",
  "an2.64-76:7.1": "66",
  "an2.64-76:8.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:8.2": "Katamāni dve?",
  "an2.64-76:8.3": "Upadhisukhañca nirupadhisukhañca.",
  "an2.64-76:8.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:8.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nirupadhisukhan”ti.",
  "an2.64-76:9.1": "67",
  "an2.64-76:10.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:10.2": "Katamāni dve?",
  "an2.64-76:10.3": "Sāsavasukhañca anāsavasukhañca.",
  "an2.64-76:10.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:10.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ anāsavasukhan”ti.",
  "an2.64-76:11.1": "68",
  "an2.64-76:12.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:12.2": "Katamāni dve?",
  "an2.64-76:12.3": "Sāmisañca sukhaṃ nirāmisañca sukhaṃ.",
  "an2.64-76:12.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:12.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nirāmisaṃ sukhan”ti.",
  "an2.64-76:13.1": "69",
  "an2.64-76:14.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:14.2": "Katamāni dve?",
  "an2.64-76:14.3": "Ariyasukhañca anariyasukhañca.",
  "an2.64-76:14.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:14.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ ariyasukhan”ti.",
  "an2.64-76:15.1": "70",
  "an2.64-76:16.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:16.2": "Katamāni dve?",
  "an2.64-76:16.3": "Kāyikañca sukhaṃ cetasikañca sukhaṃ.",
  "an2.64-76:16.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:16.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ cetasikaṃ sukhan”ti.",
  "an2.64-76:17.1": "71",
  "an2.64-76:18.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:18.2": "Katamāni dve?",
  "an2.64-76:18.3": "Sappītikañca sukhaṃ nippītikañca sukhaṃ.",
  "an2.64-76:18.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:18.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nippītikaṃ sukhan”ti.",
  "an2.64-76:19.1": "72",
  "an2.64-76:20.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:20.2": "Katamāni dve?",
  "an2.64-76:20.3": "Sātasukhañca upekkhāsukhañca.",
  "an2.64-76:20.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:20.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ upekkhāsukhan”ti.",
  "an2.64-76:21.1": "73",
  "an2.64-76:22.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:22.2": "Katamāni dve?",
  "an2.64-76:22.3": "Samādhisukhañca asamādhisukhañca.",
  "an2.64-76:22.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:22.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ samādhisukhan”ti.",
  "an2.64-76:23.1": "74",
  "an2.64-76:24.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:24.2": "Katamāni dve?",
  "an2.64-76:24.3": "Sappītikārammaṇañca sukhaṃ nippītikārammaṇañca sukhaṃ.",
  "an2.64-76:24.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:24.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ nippītikārammaṇaṃ sukhan”ti.",
  "an2.64-76:25.1": "75",
  "an2.64-76:26.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:26.2": "Katamāni dve?",
  "an2.64-76:26.3": "Sātārammaṇañca sukhaṃ upekkhārammaṇañca sukhaṃ.",
  "an2.64-76:26.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:26.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ upekkhārammaṇaṃ sukhan”ti.",
  "an2.64-76:27.1": "76",
  "an2.64-76:28.1": "“Dvemāni, bhikkhave, sukhāni.",
  "an2.64-76:28.2": "Katamāni dve?",
  "an2.64-76:28.3": "Rūpārammaṇañca sukhaṃ arūpārammaṇañca sukhaṃ.",
  "an2.64-76:28.4": "Imāni kho, bhikkhave, dve sukhāni.",
  "an2.64-76:28.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sukhānaṃ yadidaṃ arūpārammaṇaṃ sukhan”ti.",
  "an2.64-76:29.1": "Sukhavaggo dutiyo."
}