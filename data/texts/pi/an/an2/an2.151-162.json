  "an2.151-162:1.1": "Aṅguttara Nikāya 2",
  "an2.151-162:2.1": "14. Santhāravagga",
  "an2.151-162:3.1": "151",
  "an2.151-162:4.1": "“Dveme, bhikkhave, <span class=\"var\" title=\"sandhārā (mr)\" id=\"note71\">santhārā</span>.",
  "an2.151-162:4.2": "Katame dve?",
  "an2.151-162:4.3": "Āmisasanthāro ca dhammasanthāro ca.",
  "an2.151-162:4.4": "Ime kho, bhikkhave, dve santhārā.",
  "an2.151-162:4.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ santhārānaṃ yadidaṃ dhammasanthāro”ti.",
  "an2.151-162:5.1": "152",
  "an2.151-162:6.1": "“Dveme, bhikkhave, <span class=\"var\" title=\"paṭisandhārā (mr)\" id=\"note72\">paṭisanthārā</span>.",
  "an2.151-162:6.2": "Katame dve?",
  "an2.151-162:6.3": "Āmisapaṭisanthāro ca dhammapaṭisanthāro ca.",
  "an2.151-162:6.4": "Ime kho, bhikkhave, dve paṭisanthārā.",
  "an2.151-162:6.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ paṭisanthārānaṃ yadidaṃ dhammapaṭisanthāro”ti.",
  "an2.151-162:7.1": "153",
  "an2.151-162:8.1": "“Dvemā, bhikkhave, esanā.",
  "an2.151-162:8.2": "Katamā dve?",
  "an2.151-162:8.3": "Āmisesanā ca dhammesanā ca.",
  "an2.151-162:8.4": "Imā kho, bhikkhave, dve esanā.",
  "an2.151-162:8.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ esanānaṃ yadidaṃ dhammesanā”ti.",
  "an2.151-162:9.1": "154",
  "an2.151-162:10.1": "“Dvemā, bhikkhave, pariyesanā.",
  "an2.151-162:10.2": "Katamā dve?",
  "an2.151-162:10.3": "Āmisapariyesanā ca dhammapariyesanā ca.",
  "an2.151-162:10.4": "Imā kho, bhikkhave, dve pariyesanā.",
  "an2.151-162:10.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pariyesanānaṃ yadidaṃ dhammapariyesanā”ti.",
  "an2.151-162:11.1": "155",
  "an2.151-162:12.1": "“Dvemā, bhikkhave, pariyeṭṭhiyo.",
  "an2.151-162:12.2": "Katamā dve?",
  "an2.151-162:12.3": "Āmisapariyeṭṭhi ca dhammapariyeṭṭhi ca.",
  "an2.151-162:12.4": "Imā kho, bhikkhave, dve pariyeṭṭhiyo.",
  "an2.151-162:12.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pariyeṭṭhīnaṃ yadidaṃ dhammapariyeṭṭhī”ti.",
  "an2.151-162:13.1": "156",
  "an2.151-162:14.1": "“Dvemā, bhikkhave, pūjā.",
  "an2.151-162:14.2": "Katamā dve?",
  "an2.151-162:14.3": "Āmisapūjā ca dhammapūjā ca.",
  "an2.151-162:14.4": "Imā kho, bhikkhave, dve pūjā.",
  "an2.151-162:14.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pūjānaṃ yadidaṃ dhammapūjā”ti.",
  "an2.151-162:15.1": "157",
  "an2.151-162:16.1": "“Dvemāni, bhikkhave, ātitheyyāni.",
  "an2.151-162:16.2": "Katamāni dve?",
  "an2.151-162:16.3": "Āmisātitheyyañca dhammātitheyyañca.",
  "an2.151-162:16.4": "Imāni kho, bhikkhave, dve ātitheyyāni.",
  "an2.151-162:16.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ ātitheyyānaṃ yadidaṃ dhammātitheyyan”ti.",
  "an2.151-162:17.1": "158",
  "an2.151-162:18.1": "“Dvemā, bhikkhave, iddhiyo.",
  "an2.151-162:18.2": "Katamā dve?",
  "an2.151-162:18.3": "Āmisiddhi ca dhammiddhi ca.",
  "an2.151-162:18.4": "Imā kho, bhikkhave, dve iddhiyo.",
  "an2.151-162:18.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ iddhīnaṃ yadidaṃ dhammiddhī”ti.",
  "an2.151-162:19.1": "159",
  "an2.151-162:20.1": "“Dvemā, bhikkhave, vuddhiyo.",
  "an2.151-162:20.2": "Katamā dve?",
  "an2.151-162:20.3": "Āmisavuddhi ca dhammavuddhi ca.",
  "an2.151-162:20.4": "Imā kho, bhikkhave, dve vuddhiyo.",
  "an2.151-162:20.5": "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ vuddhīnaṃ yadidaṃ dhammavuddhī”ti.",
  "an2.151-162:21.1": "160",
  "an2.151-162:22.1": "“Dvemāni, bhikkhave, ratanāni.",
  "an2.151-162:22.2": "Katamāni dve?",
  "an2.151-162:22.3": "Āmisaratanañca dhammaratanañca.",
  "an2.151-162:22.4": "Imāni kho, bhikkhave, dve ratanāni.",
  "an2.151-162:22.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ ratanānaṃ yadidaṃ dhammaratanan”ti.",
  "an2.151-162:23.1": "161",
  "an2.151-162:24.1": "“Dveme, bhikkhave, sannicayā.",
  "an2.151-162:24.2": "Katame dve?",
  "an2.151-162:24.3": "Āmisasannicayo ca dhammasannicayo ca.",
  "an2.151-162:24.4": "Ime kho, bhikkhave, dve sannicayā.",
  "an2.151-162:24.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sannicayānaṃ yadidaṃ dhammasannicayo”ti.",
  "an2.151-162:25.1": "162",
  "an2.151-162:26.1": "“Dvemāni, bhikkhave, vepullāni.",
  "an2.151-162:26.2": "Katamāni dve?",
  "an2.151-162:26.3": "Āmisavepullañca dhammavepullañca.",
  "an2.151-162:26.4": "Imāni kho, bhikkhave, dve vepullāni.",
  "an2.151-162:26.5": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ vepullānaṃ yadidaṃ dhammavepullan”ti.",
  "an2.151-162:27.1": "Santhāravaggo catuttho."
}