  "an8.85:1.1": "Aṅguttara Nikāya 8",
  "an8.85:2.1": "9. Sativagga",
  "an8.85:3.1": "<span class=\\\"var\\\" title=\\\"tathāgatādhivacanasuttaṃ (bj)\\\" id=\\\"note293\\\">85. Samaṇasutta</span>",
  "an8.85:4.1": "“‘Samaṇo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa.",
  "an8.85:4.2": "‘Brāhmaṇo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa.",
  "an8.85:4.3": "‘Vedagū’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa.",
  "an8.85:4.4": "‘Bhisakko’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa.",
  "an8.85:4.5": "‘Nimmalo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa.",
  "an8.85:4.6": "‘Vimalo’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa.",
  "an8.85:4.7": "‘Ñāṇī’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassa.",
  "an8.85:4.8": "‘Vimutto’ti, bhikkhave, tathāgatassetaṃ adhivacanaṃ arahato sammāsambuddhassāti.",
  "an8.85:5.1": "Yaṃ samaṇena pattabbaṃ,",
  "an8.85:5.2": "brāhmaṇena vusīmatā;",
  "an8.85:5.3": "Yaṃ vedagunā pattabbaṃ,",
  "an8.85:5.4": "bhisakkena anuttaraṃ.",
  "an8.85:6.1": "Yaṃ nimmalena pattabbaṃ,",
  "an8.85:6.2": "vimalena sucīmatā;",
  "an8.85:6.3": "Yaṃ ñāṇinā ca pattabbaṃ,",
  "an8.85:6.4": "vimuttena anuttaraṃ.",
  "an8.85:7.1": "Sohaṃ vijitasaṅgāmo,",
  "an8.85:7.2": "mutto mocemi bandhanā;",
  "an8.85:7.3": "Nāgomhi paramadanto,",
  "an8.85:7.4": "asekho parinibbuto”ti.",
  "an8.85:8.1": "Pañcamaṃ."
}