  "an7.96-614:15.15": "(481–528.)",
  "an7.96-614:16.1": "“Chadvārārammaṇesvettha,",
  "an7.96-614:16.2": "viññāṇesu ca phassesu;",
  "an7.96-614:16.3": "Vedanāsu ca dvārassa,",
  "an7.96-614:16.4": "suttā honti visuṃ aṭṭha.",
  "an7.96-614:17.1": "Saññā sañcetanā taṇhā,",
  "an7.96-614:17.2": "vitakkesu vicāre ca;",
  "an7.96-614:17.3": "Gocarassa visuṃ aṭṭha,",
  "an7.96-614:17.4": "pañcakkhandhe ca pacceke.",
  "an7.96-614:18.1": "Soḷasasvettha mūlesu,",
  "an7.96-614:18.2": "aniccaṃ dukkhamanattā;",
  "an7.96-614:18.3": "Khayā vayā virāgā ca,",
  "an7.96-614:18.4": "nirodhā paṭinissaggā.",
  "an7.96-614:19.1": "Kamaṃ aṭṭhānupassanā,",
  "an7.96-614:19.2": "yojetvāna visuṃ visuṃ;",
  "an7.96-614:19.3": "Sampiṇḍitesu sabbesu,",
  "an7.96-614:19.4": "honti pañca satāni ca;",
  "an7.96-614:19.5": "Aṭṭhavīsati suttāni,",
  "an7.96-614:19.6": "āhuneyye ca <span class=\\\"var\\\" title=\\\"imā uddānagāthāyo bj, s1-3, pts1 potthakesu\\\" id=\\\"note264\\\">vaggike</span>”.",
  "an7.96-614:20.1": "Āhuneyyavaggo dasamo."
