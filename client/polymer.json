{
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "entrypoint": "index.html",
  "extraDependencies": [
    "./localization/elements/**/*.json",
    "./styles/**/*.json",
    "./img/**/*",
    "./files/**/*",
    "./node_modules/workbox-sw/build/importScripts/*",
    "./manifest.json",
    "./service-worker.js"
  ],
  "builds": [
    {
      "name": "es2015-push",
      "bundle": {
        "stripComments": true,
        "inlineCss": true,
        "sourcemaps": true
      },
      "js": {
        "compile": false,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "addPushManifest": true,
      "browserCapabilities": ["es2015", "push"]
    },
    {
      "name": "es2015",
      "bundle": {
        "stripComments": true,
        "inlineCss": true,
        "sourcemaps": true
      },
      "js": {
        "compile": false,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "browserCapabilities": ["es2015"]
    },
    {
      "name": "default",
      "bundle": {
        "stripComments": true,
        "inlineCss": true,
        "sourcemaps": true
      },
      "js": {
        "compile": true,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false
    }

  ]
}