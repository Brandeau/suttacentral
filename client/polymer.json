  "extraDependencies": [
    "localization/elements/**/*.json",
    "img/**/*",
    "node_modules/workbox-sw/build/importScripts/*",
    "manifest.json",
    "files/**/*",
    "elements/text/transliterator.js",
    "push_manifest.json",
    "polyfills.js",
    "service-worker.js",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/*.js.map"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "bundle": {
        "stripComments": true,
        "inlineCss": true,
        "sourcemaps": false
      },
      "js": {
        "compile": true,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    },
    {
      "name": "default",
      "bundle": {
        "stripComments": true,
        "inlineCss": true,
        "sourcemaps": false
      },
      "js": {
        "compile": false,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}